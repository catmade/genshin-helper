<Window x:Class="genshin_helper.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <CheckBox Content="Mouse Pass Through" HorizontalAlignment="Left" Margin="193,131,0,0" VerticalAlignment="Top" Checked="CheckBox_Checked" Unchecked="CheckBox_Checked"/>
        <Slider HorizontalAlignment="Center" Margin="0,167,0,0" VerticalAlignment="Top" Width="140" Value="50" Maximum="100" ValueChanged="SliderOpacity_ValueChanged"/>
        <Slider HorizontalAlignment="Center" Margin="0,202,0,0" VerticalAlignment="Top" Width="140" Value="25" Maximum="50" ValueChanged="SliderRadius_ValueChanged"/>
    </Grid>
</Window>
